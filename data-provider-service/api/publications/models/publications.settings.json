{
  "kind": "collectionType",
  "collectionName": "publications",
  "info": {
    "name": "Publications",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
