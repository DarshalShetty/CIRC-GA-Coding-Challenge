      "type": "string"
    },
    "year": {
      "type": "integer"
    },
    "pages": {
      "type": "string",
      "regex": "^\\d+-\\d+$"
    },
    "authors": {
      "collection": "people"
    },
    "url": {
      "type": "string"
    }
  }
}
